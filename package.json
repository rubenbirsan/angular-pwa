{
  "name": "angular-workbox",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "postbuild": "npm run build:pwa:web-config && npm run build:pwa:web",
    "build:pwa:web-config": "powershell Copy-Item ./web.config ./dist/angular-workbox/web.config",
    "build:pwa:web": "powershell Remove-Item -Recurse -Force ./workbox/config/service-worker.js && webpack --config ./workbox/webpack.config.js && node ./workbox/workbox-build-inject.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:web:prod": "npm run build",
    "start:web:prod1": "npm run build && http-server dist/angular-workbox --c-1 -a 0.0.0.0 -p 8080"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.7",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.7",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "dexie": "^3.2.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "workbox-sw": "^7.0.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.2",
    "@angular/cli": "~16.2.2",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "http-proxy-middleware": "^2.0.6",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "path": "^0.12.7",
    "ts-loader": "^9.4.4",
    "typescript": "~5.1.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "workbox-build": "^7.0.0"
  }
}
